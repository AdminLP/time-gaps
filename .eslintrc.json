{
    "extends": "airbnb-base",
    "rules": {
        "padding-line-between-statements": [
            "error",
            { "blankLine": "always", "prev": "*", "next": ["return", "if"] }
        ]
    },
    "plugins": [
        "import"
    ],
    "env": {
        "mocha": true,
        "es6": true,
        "node": true
    }
}
